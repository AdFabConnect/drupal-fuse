    "scripts": {
        "pre-install-cmd": [
            "drush make install.make --yes"
        ],
        "post-install-cmd": [
            "php -f drupal-fuse-install.php",
            "drush site-install standard --account-name=admin --account-pass=admin --db-url=mysql://root:root@localhost/test --yes",
            "drush pm-enable jquery_update ctools date date_api devel entity field_collection field_group googleanalytics* i18n* link* pathauto* references node_reference user_reference token* variable variable_admin variable_realm variable_store webform* wysiwyg* xmlsitemap* --yes",
            "drush pm-disable overlay --yes",
            "drush cc all --yes",
            "drush site pm-enable themefuse --yes",
            "drush vset theme_default themefuse --yes",
            "drush site pm-disable bartik --yes",
            "drush site pm-disable seven --yes",
            "npm install",
            "bower install --allow-root",
            "grunt styleguide",
            "grunt less"
        ],
        "post-update-cmd": [
            "drush pm-enable jquery_update ctools date date_api devel entity field_collection field_group googleanalytics* i18n* link* pathauto* references node_reference user_reference token* variable variable_admin variable_realm variable_store webform* wysiwyg* xmlsitemap* --yes",
            "npm update",
            "bower install --allow-root",
            "grunt styleguide",
            "grunt less"
        ]
    },
    "config": {
        "vendor-dir": "var/vendor"
    },
    "minimum-stability": "dev"
}
